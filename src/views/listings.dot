{{##def.listing:
	<div class="fancy-listing {{? v.unread === true }}fancy-unread{{?}}" data-id="{{= k }}">
		<img class="fancy-avatar" src="{{! _get_avatar(v.user_id) }}" />
		<div class="fancy-text">
			<div class="fancy-title">{{= _USERS[v.user_id] }}</div>
			<div class="fancy-content">{{! v.content }}</div>
			<div class="fancy-reply fancy-content">{{! (v.replies[v.replies.length-1]||{}).content||'no replies' }}</div>
			<div class="fancy-time">{{= _timeago(v.updated) }}</div>
		</div>
	</div>
#}}

<div class="fancy-listings">
	{{? it.length }}
		{{~ it :v:k }}
			{{#def.listing}}
		{{~}}
	{{??}}
		<div class="fancy-empty">There are no threads yet.</div>
	{{?}}
</div>
<div class="fancy-powered-by">
	<a href="https://FancySupport.com">Powered by Fancy Support</a>
</div>
