{{##def.listing:
	<div class="listing {{? v.unread === true }}unread{{?}}" data-id="{{= k }}">
		<img class="avatar" src="http://dummyimage.com/50/000/fff" />
		<div class="text">
			<div class="title">{{= FancySupport.users[v.user_id] }}</div>
			<div class="content">{{! v.content }}</div>
			<div class="reply content">{{! (v.replies[v.replies.length-1]||{}).content||'no replies' }}</div>
			<div class="time">{{= FancySupport.timeago(v.updated) }}</div>
		</div>
	</div>
#}}

{{~ it :v:k }}
	{{#def.listing}}
{{~}}
